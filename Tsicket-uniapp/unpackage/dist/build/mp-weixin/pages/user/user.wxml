<view><view class="userinfo flex-column"><image class="usercard" src="../../static/cardback0.jpg"></image><block wx:if="{{hasUserInfo}}"><view><view class="padding flex align-start justify-between"><view class="cu-avatar xxl round" style="{{'background-image:'+(userInfo.avatarUrl?'url('+userInfo.avatarUrl+')':'')+';'}}"></view><view class="text-white"><view class="text-right"><view class="text-sl text-ellipsis"><text class="text-bold">{{userInfo.nickName}}</text></view></view><view class="text-right"><block wx:if="{{!hasTsinghuaInfo}}"><button data-event-opts="{{[['tap',[['identification',['$event']]]]]}}" class="cu-btn round light bg-blue" bindtap="__e">清华身份认证</button></block><block wx:else><text>{{tsinghuaid}}</text></block></view><view class="text-left flex justify-start text-sm padding"><view data-event-opts="{{[['tap',[['likePage',['$event']]]]]}}" class="padding-right-xl" bindtap="__e"><view class="text-xl text-bold">{{like}}</view>喜爱</view><view data-event-opts="{{[['tap',[['followPage',['$event']]]]]}}" class="padding-right-xl" bindtap="__e"><view class="text-xl text-bold">{{follow}}</view>关注</view><view data-event-opts="{{[['tap',[['historyPage',['$event']]]]]}}" class="padding-right-xl" bindtap="__e"><view class="text-xl text-bold">{{history}}</view>历史</view></view></view></view></view></block><block wx:else><view class="flex justify-center getinfobtn"><button class="cu-btn round" open-type="getUserInfo" data-event-opts="{{[['getuserinfo',[['getUserInfo',['$event']]]]]}}" bindgetuserinfo="__e">授权个人信息</button></view></block></view><view class="tabs"><view class="nav" current="{{current}}" data-event-opts="{{[['change',[['navChange',['$event']]]]]}}" bindchange="__e"><scroll-view class="bg-white nav" scroll-x><view class="flex text-center"><block wx:for="{{tabs}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['flex-sub tab',index==current?'tab-choose':'']}}" data-id="{{index}}" data-event-opts="{{[['tap',[['tabSelect',['$event']]]]]}}" bindtap="__e">{{''+item+''}}</view></block></view></scroll-view></view></view><view class="padding flex-column"><block wx:for="{{userActivity}}" wx:for-item="item" wx:for-index="index" wx:key="event_id"><activity-prepare class="{{[item.delay?'animation-slide-bottom':item.cancel]}}" style="{{'animation-delay:'+(item.delay)+';'}}" vue-id="{{'1-'+index}}" activity="{{item}}" data-event-opts="{{[['^clickSponsor',[['sponsorPage']]],['^detail',[['activityPage',['$0'],[[['userActivity','event_id',item.event_id,'event_id']]]]]],['^announcement',[['announcementPage']]],['^QRcode',[['QRcodePage']]],['^cancel',[['cancelActivity',[index]]]],['^like',[['likeActivity',[index]]]],['^unfold',[['unfold',[index]]]]]}}" bind:clickSponsor="__e" bind:detail="__e" bind:announcement="__e" bind:QRcode="__e" bind:cancel="__e" bind:like="__e" bind:unfold="__e" bind:__l="__l"></activity-prepare></block></view></view>