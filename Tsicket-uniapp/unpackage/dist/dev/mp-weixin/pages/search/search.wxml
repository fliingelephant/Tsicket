<view><view class="cu-bar search animation-fade"><view class="search-form round"><text class="cuIcon-search"></text><input adjust-position="{{false}}" type="text" placeholder="根据活动名,组织搜索活动" confirm-type="search" data-event-opts="{{[['confirm',[['newsearch',['$event']]]],['input',[['__set_model',['','keyword','$event',[]]]]]]}}" value="{{keyword}}" bindconfirm="__e" bindinput="__e"/></view><view class="action"><button data-event-opts="{{[['tap',[['newsearch',['$event']]]]]}}" class="cu-btn shadow-blur round" bindtap="__e">搜索</button></view></view><view class="padding"><view class="flex-column"><block wx:for="{{activitylist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><activity-mini-card class="{{[item.delay?'animation-slide-bottom':'']}}" style="{{'animation-delay:'+(item.delay)+';'}}" vue-id="{{'1-'+index}}" activity="{{item}}" data-event-opts="{{[['^like',[['like',[index]]]],['^clickCard',[['activityPage']]]]}}" bind:like="__e" bind:clickCard="__e" bind:__l="__l"></activity-mini-card></block><block wx:if="{{noresult}}"><view class="text-center text-gray">这里空空如也</view></block></view></view></view>