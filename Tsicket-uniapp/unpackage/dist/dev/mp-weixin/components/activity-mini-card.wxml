<view class="activity-mini-card"><image class="imgcard" src="{{activity.bgUrl}}"></image><view class="card-back"></view><view data-event-opts="{{[['tap',[['$emit',['clickCard','$0'],['activity.id']]]]]}}" class="info padding flex align-start justify-between text-white" bindtap="__e"><view class="text-left flex-column info-flex"><view class="text-xxl">{{activity.name+''}}<view class="line-round"></view><view class="text-df">{{activity.intro}}</view></view><view class="text-df">{{activity.start}}</view></view><view class="text-right flex-column info-flex"><view></view><text data-event-opts="{{[['tap',[['$emit',['like','$0'],['activity.id']]]]]}}" class="{{['like',activity.like?'cuIcon-likefill':'cuIcon-like']}}" catchtap="__e"></text></view></view></view>